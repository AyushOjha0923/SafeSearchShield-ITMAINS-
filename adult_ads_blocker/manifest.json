{
    "name": "Adult AD Blocker",
    "version": "1.0.01",
    "manifest_version": 3,
    "permissions": ["declarativeNetRequest"],
    "description": "Block ads using declarativenetRequest API",
    "host_permissions":["*://*/*"],
    "declarativeNetRequest":{
      "rule_resources":[
        {
          "id": "ad_blocking_rules",
          "enabled":true,
          "path":"regex.json"
        }
      ]
    }

}
